public class LinearSearch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int x[]= {1,2,3,4};
		
		int s=3;
		boolean f=false;
		
		for(int i=0;i<x.length;i++)
		{
		if(x[i]==s)
		{
		System.out.println("element found"+" "+s);
		f=true;
		}
	    }
		
		if(f==false)
		{
		System.out.println("element not found");
		}
		

	}

}
