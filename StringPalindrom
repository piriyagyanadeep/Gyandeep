public class StringPalindrom {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String s="sos";
		
		String rev="";
		
		
		
		for(int i=s.length()-1;i>=0;i--)
		{
		rev=rev+s.charAt(i);
		}
		
		if(s.equals(rev))
		{
		System.out.println(s +" is palindrom");
		}
		else
		{
		System.out.println("is not palindrom");
		}

	}

}
